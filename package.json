    "node-sass": "8.0.0",
    "sass-loader": "10.4.1",
    "vue-loader": "15.10.1",
    "vue-template-compiler": "2.5.22",
    "web-ext": "7.5.0",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
